{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-doodle": "rm -rf dist/ ; parcel build --public-url '.'",
    "zip-dist": "rm -f dist.zip; cd dist && zip -r ../dist.zip *",
    "start-doodle": "parcel",
    "build-blockstyle-rust-pkg-release": "wasm-pack build --target web blockarts/Pattern03/blockstyle --release",
    "build-blockstyle-rust-pkg-debug": "wasm-pack build --target web blockarts/Pattern03/blockstyle --dev",
    "build-blockstyle": "NODE_ENV=production webpack --mode production --config blockstyle.webpack.config.js",
    "build-blockstyle-dev": "webpack --mode development --config blockstyle.webpack.config.js",
    "mountains-circle-prod": "cd doodles/mountains-circle && rm -rf dist && webpack && cp index.html dist && cp thumbnail.png dist && rm -f dist.zip && cd dist && zip -r ../dist.zip *",
    "mountains-circle-build": "cd doodles/mountains-circle && webpack && cp index.html dist",
    "mountains-circle-start": "cd doodles/mountains-circle && http-server dist"
  },
